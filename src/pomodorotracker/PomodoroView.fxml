               <font>
                  <Font size="18.0" />
               </font></Button>
            <Button fx:id="stopButton" layoutX="159.0" layoutY="40.0" mnemonicParsing="false" onAction="#handleStopButtonAction" prefHeight="25.0" prefWidth="78.0" text="STOP">
